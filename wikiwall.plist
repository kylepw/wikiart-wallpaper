<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
	<dict>
		<key>Label</key>
		<string>wikiwall.plist</string>

		<key>StandardErrorPath</key>
		<string>/tmp/wikiwall_stderr.log</string>

    		<key>StandardOutPath</key>
		<string>/tmp/wikiwall_stdout.log</string>

		<key>Program</key>
        	<string>WIKIWALL</string>

		<key>StartCalendarInterval</key>
		<dict>
			<key>Hour</key>
			<integer>22</integer>
			<key>Minute</key>
			<integer>00</integer>
		</dict>
	</dict>
</plist>
